<?xml version="1.0" encoding="UTF-8"?>
<xliff version="1.0" xmlns:t3="http://typo3.org/schemas/xliff">
	<file source-language="en" datatype="plaintext">
		<body>

			<!-- extConf -->
			<trans-unit id="extConf.apiHookTables">
				<source>Comma-separated list of tables for which an API request should start when saving in the backend</source>
			</trans-unit>
			<trans-unit id="extConf.defaultCountry">
				<source>ISO country code of the country that is used by default for all addresses if no other value is specified</source>
			</trans-unit>
			<trans-unit id="extConf.removeFields">
				<source>Comma-separated list of fields that are to be hidden in the data record "Location".</source>
			</trans-unit>
			<trans-unit id="extConf.pluginsWithHeader">
				<source>Comma-separated list of plugins with header-field in the backend.</source>
			</trans-unit>
			<trans-unit id="extConf.pageTitleFields">
				<source>Comma-separated list of fields for generating the page title. First field with data is used.</source>
			</trans-unit>
			<trans-unit id="extConf.pageTitleSeparator">
				<source>Separator for page title.</source>
			</trans-unit>
			<trans-unit id="extConf.pageTitleIncludePageName">
				<source>Add name of website to page title (from site-configurationn, needs pageTitleSeparator set)</source>
			</trans-unit>
			<trans-unit id="extConf.pageTitleCombineFields">
				<source>If set the defined fields in pageTitleFields are combined in the order of their appearence instead of being used as fallbacks-chain (needs pageTitleSeparator set).</source>
			</trans-unit>


			<!-- Plugins -->
			<trans-unit id="plugin.tx_gadgetogoogle_map.title">
				<source>Google Map with Filters and Search, optional with List</source>
			</trans-unit>
			<trans-unit id="plugin.tx_gadgetogoogle_map.description">
				<source>Displays locations on a map, enables a radius search for addresses, and allows filtering by category. The results list can be displayed optionally.</source>
			</trans-unit>
			<trans-unit id="plugin.tx_gadgetogoogle_detail.title">
				<source>Details of a location</source>
			</trans-unit>
			<trans-unit id="plugin.tx_gadgetogoogle_detail.description">
				<source>Shows the detailed view of a single location with all available information.</source>
			</trans-unit>
			<trans-unit id="plugin.tx_gadgetogoogle_list.title">
				<source>List of locations</source>
			</trans-unit>
			<trans-unit id="plugin.tx_gadgetogoogle_list.description">
				<source>Displays all locations clearly as a list, grouped by category and with a scroll function for large amounts of data.</source>
			</trans-unit>
			<trans-unit id="plugin.tx_gadgetogoogle_teaser.title">
				<source>Teaser for locations</source>
			</trans-unit>
			<trans-unit id="plugin.tx_gadgetogoogle_teaser.description">
				<source>Displays selected locations as teasers. The locations to be displayed can be selected manually in the plugin.</source>
			</trans-unit>


			<!-- Content Element Wizard -->
			<trans-unit id="contentElementWizward.header">
				<source>Locations</source>
			</trans-unit>


			<!-- Flexform -->
			<trans-unit id="flexform.tab.common">
				<source>Common</source>
			</trans-unit>
			<trans-unit id="flexform.tab.view">
				<source>View</source>
			</trans-unit>
			<trans-unit id="flexform.tab.list">
				<source>List</source>
			</trans-unit>
			<trans-unit id="flexform.tab.search">
				<source>Search</source>
			</trans-unit>
			<trans-unit id="flexform.settings.locationCenter">
				<source>Location in center of map (otherwise map scales so that all locations are visible)</source>
			</trans-unit>
			<trans-unit id="flexform.settings.locations">
				<source>Shown locations (if empty, all locations are shown)</source>
			</trans-unit>
			<trans-unit id="flexform.settings.showSearch">
				<source>Show search</source>
			</trans-unit>
			<trans-unit id="flexform.settings.showList">
				<source>Show list</source>
			</trans-unit>
			<trans-unit id="flexform.settings.hideFilter">
				<source>Hide filter for pins</source>
			</trans-unit>
			<trans-unit id="flexform.settings.detailPage">
				<source>Detail page with plugin</source>
			</trans-unit>
			<trans-unit id="flexform.settings.listPage">
				<source>List page with plugin</source>
			</trans-unit>
			<trans-unit id="flexform.settings.maxSearchRadius">
				<source>Maximum radius for search (in kilometers, 0 = unlimited)</source>
			</trans-unit>
			<trans-unit id="flexform.settings.maxSearchDisplayRadius">
				<source>Maximum radius of results that is shown on the map (in kilometers, 0 = unlimited)</source>
			</trans-unit>
			<trans-unit id="flexform.settings.defaultCountry">
				<source>Default Country for search (e.g. "DE")</source>
			</trans-unit>
			<trans-unit id="flexform.settings.maxResultsPerPage">
				<source>Maximum number of results per Page</source>
			</trans-unit>
			<trans-unit id="flexform.settings.maxPages">
				<source>Maximum number of pages shown</source>
			</trans-unit>
			<trans-unit id="flexform.settings.listStyle">
				<source>List style</source>
			</trans-unit>
			<trans-unit id="flexform.settings.listStyle.default">
				<source>Default</source>
			</trans-unit>
			<trans-unit id="flexform.settings.listStyle.teaser">
				<source>Teaser</source>
			</trans-unit>
			<trans-unit id="flexform.settings.listStyle.category">
				<source>Group by category</source>
			</trans-unit>
			<trans-unit id="flexform.settings.hidePagination">
				<source>Hide pagination</source>
			</trans-unit>
			<trans-unit id="flexform.settings.paginationStyle">
				<source>Pagination style</source>
			</trans-unit>
			<trans-unit id="flexform.settings.paginationStyle.default">
				<source>Default</source>
			</trans-unit>
			<trans-unit id="flexform.settings.paginationStyle.more">
				<source>Show more</source>
			</trans-unit>
			<trans-unit id="flexform.settings.layout">
				<source>Layout</source>
			</trans-unit>
			<trans-unit id="flexform.settings.layout.default">
				<source>Default</source>
			</trans-unit>
			<trans-unit id="flexform.settings.layout.big">
				<source>Big</source>
			</trans-unit>
			<trans-unit id="flexform.settings.orderByFirst">
				<source>Order by (First)</source>
			</trans-unit>
			<trans-unit id="flexform.settings.orderBySecond">
				<source>Order by (Second)</source>
			</trans-unit>
			<trans-unit id="flexform.settings.orderBy.labelAsc">
				<source>Label, ascending</source>
			</trans-unit>
			<trans-unit id="flexform.settings.orderBy.labelDesc">
				<source>Label, descending</source>
			</trans-unit>
			<trans-unit id="flexform.settings.orderBy.streetAsc">
				<source>Street, ascending</source>
			</trans-unit>
			<trans-unit id="flexform.settings.orderBy.streetDesc">
				<source>Street, descending</source>
			</trans-unit>
			<trans-unit id="flexform.settings.orderBy.cityAsc">
				<source>City, ascending</source>
			</trans-unit>
			<trans-unit id="flexform.settings.orderBy.cityDesc">
				<source>City, descending</source>
			</trans-unit>
		</body>
	</file>
